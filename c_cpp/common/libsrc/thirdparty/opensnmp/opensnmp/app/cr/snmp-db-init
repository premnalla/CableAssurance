#!/bin/sh

if [ $# -ne 1 ]
then
	echo "$0 <database-directory>"
	exit
fi

DIR=$1
echo "creating/updating database in $DIR"

####################################################################
#
# script to run snmp-db-add to populate database
# with initial values
#
# general format:
#
# snmp-db-add <path-to-db> <oid> <bertype> <value>
#
#  OR
#
# snmp-db-add <path-to-db> <tbl.Entry>.0.<row-index> c <col> <bertype> <value>
#
####################################################################
#
# vacmSecurityToGroupEntry 3.newUser
#
../../tools/snmp-db-add/snmp-db-add $DIR \
.1.3.6.1.6.3.16.1.2.1.0.3.7.110.101.119.85.115.101.114 \
c 3 s aGroup \
c 4 i 5 \
c 5 i 1
#
####################################################################
#
# accessTable
#
# "aGroup"."".3.1
../../tools/snmp-db-add/snmp-db-add $DIR \
.1.3.6.1.6.3.16.1.4.1.0.6.97.71.114.111.117.112.0.3.1 \
c 4 i 1 \
c 5 s testview \
c 6 s testview \
c 7 s testview \
c 8 i 5 \
c 9 i 1
#
####################################################################
#
# MIBViews
#
# "testview".4.1.3.6.1
../../tools/snmp-db-add/snmp-db-add $DIR \
.1.3.6.1.6.3.16.1.5.2.1.0.8.116.101.115.116.118.105.101.119.4.1.3.6.1 \
c 3 x F0 \
c 4 i 1 \
c 5 i 5 \
c 6 i 1
#
#"testview".8.1.3.6.1.2.1.1.5
../../tools/snmp-db-add/snmp-db-add $DIR \
.1.3.6.1.6.3.16.1.5.2.1.0.8.116.101.115.116.118.105.101.119.8.1.3.6.1.2.1.1.5 \
c 3 x FF \
c 4 i 2 \
c 5 i 5 \
c 6 i 1
#
####################################################################
#
# usm user table
#
# ".....TEST"."newUser"
../../tools/snmp-db-add/snmp-db-add $DIR \
.1.3.6.1.6.3.15.1.2.2.1.0.9.128.0.7.229.4.84.69.83.84.7.110.101.119.85.115.101.114 \
c 5 o .1.3.6.1.6.3.10.1.1.2 \
c 8 o .1.3.6.1.6.3.10.1.2.2 \
c 11 s "" \
c 12 i 5 \
c 13 i 1 \
c -1 x "278cd1f90cc17ffb08ea3b59091405bf" \
c -2 x "531051bd1e2689eb4b718f64760f3949"
# "localEngineID"."fomike"
../../tools/snmp-db-add/snmp-db-add $DIR \
.1.3.6.1.6.3.15.1.2.2.1.0.13.108.111.99.97.108.69.110.103.105.110.101.73.68.6.102.111.109.105.107.101 \
c 5 o .1.3.6.1.6.3.10.1.1.1 \
c 8 o .1.3.6.1.6.3.10.1.2.1 \
c 11 s "" \
c 12 i 5 \
c 13 i 1 \
c -1 s "" \
c -2 s ""
# "localEngineID"."initial"
../../tools/snmp-db-add/snmp-db-add $DIR \
.1.3.6.1.6.3.15.1.2.2.1.0.13.108.111.99.97.108.69.110.103.105.110.101.73.68.7.105.110.105.116.105.97.108 \
c 5 o .1.3.6.1.6.3.10.1.1.1 \
c 8 o .1.3.6.1.6.3.10.1.2.1 \
c 11 s "" \
c 12 i 5 \
c 13 i 1 \
c -1 s "" \
c -2 s ""
# "localEngineID"."newUser"
../../tools/snmp-db-add/snmp-db-add $DIR \
.1.3.6.1.6.3.15.1.2.2.1.0.13.108.111.99.97.108.69.110.103.105.110.101.73.68.7.110.101.119.85.115.101.114 \
c 5 o .1.3.6.1.6.3.10.1.1.2 \
c 8 o .1.3.6.1.6.3.10.1.2.2 \
c 11 s "" \
c 12 i 5 \
c 13 i 1 \
c -1 x "278cd1f90cc17ffb08ea3b59091405bf" \
c -2 x "531051bd1e2689eb4b718f64760f3949"
# "localEngineID"."initial_auth"
../../tools/snmp-db-add/snmp-db-add $DIR \
.1.3.6.1.6.3.15.1.2.2.1.0.13.108.111.99.97.108.69.110.103.105.110.101.73.68.12.105.110.105.116.105.97.108.95.97.117.116.104 \
c 5 o .1.3.6.1.6.3.10.1.1.1 \
c 8 o .1.3.6.1.6.3.10.1.2.1 \
c 11 s "" \
c 12 i 5 \
c 13 i 1 \
c -1 s "" \
c -2 s ""
####################################################################

