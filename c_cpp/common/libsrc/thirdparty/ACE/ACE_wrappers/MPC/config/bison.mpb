// bison.mpb,v 1.3 2004/12/06 15:42:38 elliott_c Exp
project {
  Define_Custom(BISON) {
    automatic        = 1
    command          = bison
    commandflags     = -d
    pch_postrule     = 1
    inputext         = .yy, .y
    pre_extension    = .tab
    source_outputext = .c, .cpp, .cxx, .cc, .C
    header_outputext = .h, .hpp, .hxx, .hh
  }
}
