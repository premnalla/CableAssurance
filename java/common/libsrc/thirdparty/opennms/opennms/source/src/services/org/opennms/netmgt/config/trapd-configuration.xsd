<?xml version="1.0"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://xmlns.opennms.org/xsd/config/trapd">

	<element name="trapd-configuration">
		<complexType>
			<!--- the port on which traps are listened to, default value is 162 -->
			<attribute name="snmp-trap-port" type="positiveInteger" use="required"/>
			<!--- whether traps from unknown devices should generate newSuspect events -->
			<attribute name="new-suspect-on-trap" type="boolean" use="required"/>
		</complexType>
	</element>
</schema>
